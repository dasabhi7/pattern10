#include <stdio.h>
int main()
{
    int n;
    scanf("%d",&n);
    int k=1;
    while(k<=n){
        int space=n;
        while(space>k){
            printf(" ");
            space--;
        }
        int a=1;
        while(a<=k){
            printf("%d",a);
            a++;
        }
        printf("\n");
        k++;
    }
}
